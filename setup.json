{
    "author": "Alec Mitchell",
    "author_email": "alecwmitchell@gmail.com",
    "classifiers": [
        "Framework :: AiiDA",
        "License :: OSI Approved :: MIT License",
        "Programming Language :: Python :: 3.6",
        "Programming Language :: Python :: 3.7",
        "Programming Language :: Python :: 3.8",
        "Programming Language :: Python :: 3.9",
        "Development Status :: 5 - Production/Stable"
    ],
    "description": "The unofficial AiiDA plugin for Dakota",
    "entry_points": {
        "aiida.calculations": [
            "dakota.study = aiida_dakota.calculations.study:StudyCalculation"
        ],
        "aiida.parsers": [
            "dakota.study = aiida_dakota.parsers.study:StudyParser"
        ],
        "aiida.tools.calculations": [
            "dakota.study = aiida_dakota.tools.calculations.study:StudyCalculationTools"
        ],
        "aiida.workflows": [
            "dakota.study.base = aiida_dakota.workflows.study.base:StudyBaseWorkChain"
        ],
        "console_scripts": [
            "aiida-dakota = aiida_dakota.cli:cmd_root"
        ]
    },
    "extras_require": {
        "pre-commit": [
            "pre-commit~=2.2",
            "pylint~=2.6.0"
        ],
        "tests": [
            "pgtest~=1.3",
            "pytest~=6.0",
            "pytest-regressions~=1.0"
        ],
        "docs": [
            "Sphinx",
            "docutils",
            "sphinx-copybutton~=0.3.0",
            "sphinx-book-theme~=0.1.0",
            "sphinx-click~=2.7.1"
        ]
    },
    "install_requires": [
        "psycopg2-binary<2.9",
        "aiida_core[atomic_tools]~=1.4,>=1.4.4",
        "jsonschema",
        "packaging",
        "qe-tools~=2.0rc1",
        "xmlschema~=1.2,>=1.2.5",
        "numpy",
        "importlib_resources"
    ],
    "license": "MIT License",
    "name": "aiida_dakota",
    "python_requires": ">=3.6",
    "url": "https://github.com/awmitch/aiida-dakota",
    "version": "0.0.1"
}
